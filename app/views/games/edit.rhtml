<h1>Editing game</h1>

<%= error_messages_for :game %>

<% form_for(:game, :url => game_path(@theme,@game), :html => { :method => :put }) do |f| %>


<%= render :partial => 'form', :locals => {:f => f} %>
  <p>
    <%= submit_tag "Update" %>
  </p>
<% end %>

<%= link_to 'Show', game_path(@theme,@game) %> |
<%= link_to 'Back', games_path %>
