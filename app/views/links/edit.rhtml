<h1>Editar Lista</h1>

<% form_for(:link, :url => link_path(@theme, @link), :html => { :method => :put }) do |f| %>
  
  <%= render :partial => 'form', :locals => {:f => f} %>
<p>
    <%= submit_tag "Update" %>
  </p>
<% end %>

<%= link_to 'Show', link_path(@theme, @link) %> |
<%= link_to 'Back', links_path %>
