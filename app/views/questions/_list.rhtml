<% for activity in activities %>
<p id="ques_<%= activity.creation_id %>">
[<%= activity.creation.answers.count %>]
<%= link_to_remote "#{activity.creation.text[0...15]}...", :url => group_question_url(@group.id, activity.creation_id), :method => 'get'%> - <%= activity.interactions.last.created_at.to_s(:interaction) %>
<% end %>
